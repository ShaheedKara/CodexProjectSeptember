@model IEnumerable<Sprint1AppDev3A.Models.RFQ>

@{
    ViewBag.Title = "AgentIndex";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Incoming Requests</h2>
<p>
