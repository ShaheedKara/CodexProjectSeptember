            @Html.DisplayNameFor(model => model.DelCity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DelCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PUDepot)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Instruct)
        </th>
        @*<th>
            @Html.DisplayNameFor(model => model.price)
        </th>*@
       
        <th></th>
    </tr>

@foreach (var item in Model) {
