    {
        @Html.Label("Search For a Email :   ")    @Html.TextBox("s")
        <input type="submit" value="Search" />
    }
</p>

<table class="table">
    <tr>
        @*<th>
            @Html.DisplayNameFor(model => model.Size)
        </th>*@
